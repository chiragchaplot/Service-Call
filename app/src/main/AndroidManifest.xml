<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tseapp.paperbind.servicecall" >
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".todolist"
            android:label="@string/title_activity_todolist" >
        </activity>
        <activity
            android:name=".Today_List_Work"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_today__list__work" >
        </activity>
        <activity
            android:name=".service_start"
            android:screenOrientation="portrait"

            android:label="@string/title_activity_service_start" >
        </activity>
        <activity
            android:name=".fill_details"
            android:label="@string/title_activity_fill_details" >
        </activity>
        <activity
            android:name=".counter"
            android:screenOrientation="portrait"

            android:label="@string/title_activity_counter" >
        </activity>
        <activity
            android:name=".knowledge_base"
            android:screenOrientation="portrait"

            android:label="@string/title_activity_knowledge_base" >
        </activity>
        <activity
            android:name=".chose_location"
            android:screenOrientation="portrait"

            android:label="@string/title_activity_chose_location" >
        </activity>
        <activity
            android:name=".signature"
            android:screenOrientation="portrait"

            android:label="@string/title_activity_signature" >
        </activity>
        <activity
            android:name=".master_view"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_master_view" >
        </activity>
        <activity
            android:name=".welcome_screen"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"
            android:label="PaperBind TSE" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
